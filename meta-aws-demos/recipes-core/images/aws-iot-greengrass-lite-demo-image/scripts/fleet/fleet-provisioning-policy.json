{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iot:CreateThing",
        "iot:CreateThingGroup",
        "iot:AddThingToThingGroup",
        "iot:AttachThingPrincipal",
        "iot:CreatePolicy",
        "iot:AttachPolicy",
        "iot:CreateCertificateFromCsr",
        "iot:RegisterCertificate"
      ],
      "Resource": "*"
    }
  ]
}
